{
  "extends": "./base.json",
  "compilerOptions": {
    "paths": {
      "@adapter/vue": [
        "packages/adapter/vue/vue3"
      ],
      "@adapter/vue/*": [
        "packages/adapter/vue/vue3/*"
      ],
      "@adapter/hooks": [
        "packages/adapter/hooks/vue3"
      ],
      "@adapter/hooks/*": [
        "packages/adapter/hooks/vue3/*"
      ],
      "@adapter/utils": [
        "packages/adapter/utils/vue3"
      ],
      "@adapter/utils/*": [
        "packages/adapter/utils/vue3/*"
      ],
      "@td/components": [
        "packages/tdesign-vue-next"
      ],
      "@td/components/*": [
        "packages/tdesign-vue-next/src/*"
      ]
    }
  }
}
